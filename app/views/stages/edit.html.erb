<h1>Editing stage</h1>

<% form_for(@stage) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :festival_ID %><br />
    <%= f.text_field :festival_ID %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @stage %> |
<%= link_to 'Back', stages_path %>